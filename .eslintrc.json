{
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
    },
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:prettier/recommended",
        "eslint:recommended"
    ]
}